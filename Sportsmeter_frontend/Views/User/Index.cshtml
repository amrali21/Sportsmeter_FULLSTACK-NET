<div className='wrapper'>
            <div id="divider">
                <section id="memos">
                    <div class="table-responsive-xl">
            <table class="table">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                        @foreach (var user in @ViewBag.Users)
                        {
                      <tr>
                        <td>@user.FirstName</td>
                        <td>@user.LastName</td>
                        <td>@user.Email</td>
                                                <td>@user.Role</td>
                    </tr>
                        }
                </tbody>
            </table>
        </div>
                </section>
            </div>
        </div>